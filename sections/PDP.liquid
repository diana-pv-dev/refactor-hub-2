  {% liquid
    assign product_available = true
    assign rtl = false
    assign products_cart = cart.items | where: 'variant_id', product.selected_or_first_available_variant.id
    if product.selected_or_first_available_variant.available == false or products_cart[0].quantity == product.selected_or_first_available_variant.inventory_quantity
      assign product_available = false
    endif

    assign accordions = section.blocks | where: 'type', 'accordion'

    if localization.language.iso_code == 'ar'
      assign rtl = true
    endif
  %}

  {%- capture config -%}
	{
		"recommended_products": {{ product.metafields.custom.bought_together.value | json }}
	}
{%- endcapture -%}

  <s-main-product
    class="s-product js-product {% if rtl %}s-product--direction--rtl{% endif %}"
    data-section-id="{{ section.id }}"
    data-handle="{{ product.handle }}"
    data-product-id="{{ product.id }}"
    data-config="{{ config | strip_newlines | strip | escape_once }}"
  >
    {% render 'assaf-breadcrumbs' %}
    <div class="s-product__sliders">
      {% render 'assaf-product-gallery', product: product %}
      {% render 'assaf-product-gallery-zoom', product: product %}
    </div>
    <div class="s-product__container">
      <div class="s-product__content">
        <div class="s-product__discount s-product__discount--hide js-product__discount">
          <span class="s-product__discount-text">
            {{ 'assaf.sections.product.discount-text' | t }}
          </span>
          <span class="s-product__discount-timer js-product__discount-timer"> </span>
        </div>
        <div class="s-product__info">
          <div class="s-product__tag-list">
            {% for tag in product.tags %}
              {% assign tag_array = tag | split: ':' %}
              {% if tag_array[0] == 'label' %}
                <div class="s-product__tag">
                  {{ tag_array[1] }}
                </div>
              {% endif %}
            {% endfor %}
          </div>

          <h1 class="s-product__title">{{ product.title }}</h1>

          {% comment %} First Collection {% endcomment %}
          {% if product.collections.size > 0 %}
            <a href="{{ product.collections[0].url }}" class="s-product__collection">
              {{ product.collections[0].title }}
            </a>
          {% endif %}

          <div class="s-product__price-wrapper js-product__price-wrapper">
            <div class="s-product__product-price-outer">
              {%- if product.selected_or_first_available_variant.compare_at_price -%}
                <span class="s-product__product-price s-product__product-price--new">
                  {{ product.selected_or_first_available_variant.price | money_without_trailing_zeros }}
                </span>
                <span class="s-product__product-price s-product__product-price--compare-at-price">
                  {{ product.selected_or_first_available_variant.compare_at_price | money_without_trailing_zeros }}
                </span>
              {%- else -%}
                <span class="s-product__product-price">
                  {{ product.selected_or_first_available_variant.price | money_without_trailing_zeros }}
                </span>
              {%- endif -%}

              {% if product.selected_or_first_available_variant.taxable == true %}
                <span class="s-product__tax">{{ 'assaf.sections.product.vat' | t }}</span>
              {% else %}
                <span class="s-product__tax">{{ 'assaf.sections.product.no_vat' | t }}</span>
              {% endif %}
            </div>

            <span class="s-product__sku">
              {% if product.selected_or_first_available_variant.sku != blank %}
                {{ 'assaf.sections.product.sku' | t -}}
                {{- product.selected_or_first_available_variant.sku }}
              {% endif %}
            </span>
          </div>
        </div>

        <div class="s-product__payment-list">
          <div class="s-product__payment">
            <div class="s-product__payment-logo">
              <svg
                width="39"
                height="8"
                viewBox="0 0 39 8"
                fill="none"
                xmlns="http://www.w3.org/2000/svg"
                xmlns:xlink="http://www.w3.org/1999/xlink"
              >
                <rect width="39" height="8" fill="url(#pattern0)"/>
                <defs>
                <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                <use xlink:href="#image0_3_887" transform="matrix(0.000489956 0 0 0.00242131 -0.430671 0)"/>
                </pattern>
                <image id="image0_3_887" width="2920" height="413" xlink:href="data:image/png;base64,"/>
                </defs>
              </svg>
            </div>
            <p class="s-product__payment-text">Split to 4 payments of SAR 32.25 (Interest-free)</p>
            <a href="#" class="s-product__payment-link"> Learn More </a>
          </div>
        </div>

        {% render 'assaf-variant-picker', product: product %}

        <ul class="s-product__highlight-list">
          <li class="s-product__highlight">
            <div class="s-product__highlight-img">
              {% render 'icon-gift' %}
            </div>
            <h5 class="s-product__highlight-title">
              {{ 'assaf.sections.product.highlight.gift_title_html' | t }}
            </h5>
            <p class="s-product__highlight-text">
              {{ 'assaf.sections.product.highlight.gift_text_html' | t }}
            </p>
          </li>

          <li class="s-product__highlight">
            <div class="s-product__highlight-img">
              {% render 'icon-returns' %}
            </div>
            <h5 class="s-product__highlight-title">
              {{ 'assaf.sections.product.highlight.returns_title_html' | t }}
            </h5>
            <p class="s-product__highlight-text">
              {{ 'assaf.sections.product.highlight.returns_text_html' | t }}
            </p>
          </li>
        </ul>

        <div class="s-product__accordions">
          {% for accordion in accordions %}
            {% liquid
              assign title = accordion.settings.title
              assign subtitle = accordion.settings.subtitle

              if rtl
                assign title = accordion.settings.ar_title | default: accordion.settings.title
                assign subtitle = accordion.settings.ar_subtitle | default: accordion.settings.subtitle
              endif

              if accordion.settings.product_description == true
                assign subtitle = product.description
              endif
            %}
            <div
              class="s-product__accordion-item js-product__accordion-item"
              data-accordion-id="{{- forloop.index -}}"
              data-title="{{ title }}"
            >
              {{ title }}
              {% render 'icon-right-arrow' %}

              {%- comment -%}we cannot transfer content through data attribute, hence why we need this span{%- endcomment -%}
              <span style="display: none;" class="js-product__accordion-content">
                {{ subtitle }}
              </span>
            </div>
          {% endfor %}
        </div>
        {% if product.metafields.custom.bought_together.value != blank and product_available %}
          <div class="s-product__relateds-outer">
            <h3 class="s-product__relateds-title">
              {{ 'assaf.sections.product.related-title' | t }}
            </h3>

            <form class="s-product__relateds js-product__relateds">
              {% for r_product in product.metafields.custom.bought_together.value %}
                {% liquid
                  assign r_variant = r_product.selected_or_first_available_variant
                  assign option1 = r_variant.option1
                  assign option2 = r_variant.option2
                  assign option3 = r_variant.option3
                %}
                <div class="s-product__related js-product__related">
                  <label class="s-product__related-checkbox">
                    <input
                      name="selected-{{- r_product.handle -}}"
                      class="s-product__checkbox js-product__checkbox"
                      type="checkbox"
                    >
                    <span></span>
                  </label>
                  <a href="{{ r_product.url }}" class="s-product__related-image">
                    {% render 'image-lazy', image: r_product.featured_image %}
                  </a>
                  <div class="s-product__related-wrapper">
                    <a
                      href="{{ r_product.url }}"
                      class="s-product__related-title"
                    >
                      {{ r_product.title }}
                    </a>
                    <div class="s-product__related-price">
                      <span>{{ r_variant.price | money_without_trailing_zeros }}</span>
                    </div>

                    <ul class="s-product__related-options">
                      <span class="s-product__related-variant-title">{{ 'assaf.sections.product.size' | t }}</span>
                      {% for option in r_product.options_with_values %}
                        {% assign index = forloop.index %}
                        {% if option.name == 'Size' %}
                          <select
                            class="s-product__related-variant js-product__related-variant"
                            name="variant-{{- r_product.handle -}}"
                          >
                            {% for option_value in option.values %}
                              {% liquid
                                for variant in r_product.variants
                                  case index
                                    when 1
                                      if variant.option2 == option2 and variant.option3 == option3 and variant.options contains option_value
                                        assign r_option_variant = variant
                                      endif
                                    when 2
                                      if variant.option1 == option1 and variant.option3 == option3 and variant.options contains option_value
                                        assign r_option_variant = variant
                                      endif
                                    when 3
                                      if variant.option1 == option1 and variant.option2 == option2 and variant.options contains option_value
                                        assign r_option_variant = variant
                                      endif
                                  endcase
                                endfor
                              %}
                              <option
                                value="{{ r_option_variant.id }}"
                                {% if r_option_variant == r_variant %}
                                  selected
                                {% endif %}
                                {% if r_option_variant.available == false %}
                                  disabled
                                {% endif %}
                              >
                                {{ option_value }}
                              </option>
                            {% endfor %}
                          </select>
                        {% endif %}
                      {% endfor %}
                    </ul>
                  </div>
                </div>
              {% endfor %}
            </form>
          </div>
        {% endif %}
      </div>
    </div>
  </s-main-product>
