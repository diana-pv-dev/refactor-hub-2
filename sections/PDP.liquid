                    </summary>
                    <ul class="s-main-product__select-list">
                      {% for option in product_option_color.values %}
                        {% liquid
                          assign value = option | handleize

                          if selected_variant_size
                            for variant in product.variants
                              if variant.options contains option and variant.options contains selected_variant_size
                                assign color_variant = variant
                                break
                              endif
                            endfor
                          else
                            for variant in product.variants
                              if variant.options contains option
                                assign color_variant = variant
                                break
                              endif
                            endfor
                          endif
                        %}

                        <li
                          class="s-main-product__select-list-item js-main-product__select-list-item"
                          {% unless color_variant.available %}
                            disabled
                          {% endunless %}
                        >
                          <label
                            class="s-main-product__select-list-item-label"
                            for="{{ product.id }}-{{ value }}-{{ forloop.index0 }}"
                          >
                            {{ value }}
                          </label>
                        </li>
                      {% endfor %}
                    </ul>
                  </details>
                </div>
              {% endif %}

              <div class="s-main-product__form-loader"></div>

              <div class="s-main-product__form-submit-wrapper js-main-product__form-submit-wrapper">
                <button
                  class="s-main-product__form-submit js-main-product__form-submit {% if max-qty < 1 %}disabled{% endif %}"
                  type="button"
                >
                  <span>{{ 'sections.main-product.buy_button' | t }}</span>
                </button>
              </div>
            {% endform %}
          </div>

          {% comment %} Product Info {% endcomment %}
          <div class="s-main-product__info">
            {% if product.metafields.custom.product_details != blank %}
              <div class="s-main-product__details">
                <button class="s-main-product__details-button js-main-product__open-modal js-main-product__details">
                  <span class="s-main-product__details-title">{{ 'sections.main-product.details' | t }}</span>
                  {% render 'icon-arrow-right' %}
                </button>
                <div class="s-main-product__accordeon-content">
                  <p>{{ product.metafields.custom.product_details }}</p>
                </div>
              </div>
            {% endif %}
            <button class="s-main-product__details-button js-main-product__open-modal js-main-product__delivery">
              <span class="s-main-product__details-link js-main-product__details-delivery">
                {{- 'sections.main-product.delivery' | t -}}
              </span>
              {% render 'icon-arrow-right' %}
            </button>

            <button class="s-main-product__details-button js-main-product__open-modal js-main-product__help">
              <span class="s-main-product__details-link js-main-product__details-help">
                {{- 'sections.main-product.help' | t -}}
              </span>
              {% render 'icon-arrow-right' %}
            </button>
          </div>
          <div class="s-main-product__try-on">
            <button class="s-main-product__try-on-button">
              <div class="s-main-product__easysize-wrapper">
                <span class="s-main-product__easysize-icon">
                  {% render 'icon-ruler' %}
                </span>
                <span class="easysize-container"></span>
              </div>
              <span class="s-main-product__easysize-arrow">
                {% render 'icon-arrow-right' %}
              </span>
            </button>
            <span class="s-main-product__try-on-description">
              {{ section.settings.try_on_subtitle }}
            </span>
          </div>
        </div>
      </div>
    </div>
  </s-main-product>
