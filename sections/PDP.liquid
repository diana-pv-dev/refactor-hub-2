    	padding-top: {{ section.settings.top-padding_mobile }}px;
    	padding-bottom: {{ section.settings.bottom-padding_mobile }}px;
    }

    @media screen and (min-width: 768px) {
    	#shopify-section-{{ section.id }} .s-main-product {
    		padding-top: {{ section.settings.top-padding }}px;
    		padding-bottom: {{ section.settings.bottom-padding }}px;
    	}
    }
  {% endstyle %}

  {% assign selected_variant = product.selected_or_first_available_variant %}
  {% for product_option in product.options_with_values %}
    {% assign product_option_name = product_option.name | handleize %}
    {% case product_option_name %}
      {% when 'color', 'colour' %}
        {% assign color_option_position = product_option.position | minus: 1 %}
        {% assign selected_variant_color = selected_variant.options[color_option_position] %}
      {% when 'size', 'Size' %}
        {% assign size_option_position = product_option.position | minus: 1 %}
        {% assign selected_variant_size = selected_variant.options[size_option_position] %}
    {% endcase %}
  {% endfor %}

  {% liquid
    assign images_by_alt = false
    for option in product.options_by_name.Color.values
      assign option = option | handleize

      for variant_option in selected_variant.options
        assign variant_option = variant_option | handleize

        if option == variant_option
          assign image_alt = variant_option | handleize
        endif
      endfor
    endfor
  %}

  <s-main-product
    class="s-main-product"
    data-product-handle="{{ product.handle }}"
    data-product-id="{{ product.id }}"
    data-product-variant-id="{{ selected_variant.id }}"
    data-section-id="{{ section.id }}"
    {% if color_option_position %}
      data-color-option-position="{{ color_option_position }}"
    {% endif %}
    {% if size_option_position %}
      data-size-option-position="{{ size_option_position }}"
    {% endif %}
  >
    <div class="s-main-product__inner">
      {% comment %} Gallery {% endcomment %}
      {% liquid
        assign slide_image_index = 1
        assign image_count = product.media | size
      %}

      <div class="s-main-product__gallery js-main-product__gallery">
        <div class="s-main-product__gallery-image-container--first">
          <div class="s-main-product__gallery-image js-main-product__gallery-image" data-index-number="2">
            {% liquid
              for image in product.images
                assign current_image_alt = image.alt | handleize
                if current_image_alt == image_alt
                  assign images_by_alt = true
                  assign first_image = image
                  break
                else
                  assign first_image = product.images[0]
                endif
              endfor
            %}

            {% render 'image-lazy', image: first_image %}
          </div>

          <div class="s-main-product__breadcrumbs-wrapper">
            {% render 'breadcrumbs' %}
          </div>

          <div class="s-main-product__counter-wrapper-desktop">
            <div class="s-main-product__counter-desktop">
              <span
                class="s-main-product__slider-counter s-main-product__slider-counter--active js-main-product__counter-active"
              ></span>
              <span>&nbsp;/&nbsp;</span>
              <span
                class="s-main-product__slider-counter s-main-product__slider-counter--gray js-main-product__counter-total"
              ></span>
            </div>
          </div>
        </div>

        <div class="s-main-product__gallery-image-container--last {% if image_count == 2 %}lock-scroll{% endif %}">
          {% if image_count == 2 %}
            <div
              class="s-main-product__gallery-image js-main-product__gallery-image js-main-product__gallery-last-image"
            >
              {% render 'image-lazy', image: first_image %}
            </div>
          {% else %}
            {% for item in product.images %}
              {% assign current_image_alt = item.alt | handleize %}
              {% if images_by_alt %}
                {% if current_image_alt == image_alt %}
                  <div
                    class="s-main-product__gallery-image js-main-product__gallery-image js-main-product__gallery-last-image"
                    data-index-number="{{ slide_image_index }}"
                  >
                    {% render 'image-lazy', image: item %}
                    {% assign slide_image_index = slide_image_index | plus: 1 %}
                  </div>
                {% endif %}
              {% else %}
                <div
                  class="s-main-product__gallery-image js-main-product__gallery-image js-main-product__gallery-last-image"
                  data-index-number="{{ slide_image_index }}"
                >
                  {% render 'image-lazy', image: item %}
                  {% assign slide_image_index = slide_image_index | plus: 1 %}
                </div>
              {% endif %}
            {% endfor %}
          {% endif %}
        </div>

        <div class="s-main-product__counter">
          <span
            class="s-main-product__slider-counter s-main-product__slider-counter--active js-main-product__counter-active"
          ></span>
          <span>&nbsp;/&nbsp;</span>
          <span
            class="s-main-product__slider-counter s-main-product__slider-counter--gray js-main-product__counter-total"
          ></span>
        </div>
      </div>

      {% comment %} Gallery zoom {% endcomment %}
      {% render 'c-product-gallery-zoom', product_media: product.media, image_alt: image_alt %}

      {% comment %} Main {% endcomment %}
      <div class="s-main-product__main-container">
        <div class="s-main-product__panel-holder"></div>
        <div class="s-main-product__main js-main-product__main">
          <div class="s-main-product__main-top">
            {% comment %} Title {% endcomment %}
            <div class="s-main-product__title">{{ product.title }}</div>
            {% comment %} Wishlist button {% endcomment %}
            <div class="s-main-product__wishlist js-main-product__wishlist">
              {% render 'icon-wishlist' %}
            </div>
          </div>
          {% comment %} Price {% endcomment %}
          <div class="s-main-product__price">
            {%- if selected_variant.compare_at_price -%}
              <span class="s-main-product__price s-main-product__price--discount">
                {{ selected_variant.price | money_without_trailing_zeros }}
              </span>
              <span class="s-main-product__compare-at-price">
                {{ selected_variant.compare_at_price | money_without_trailing_zeros }}
              </span>
            {%- else -%}
              <span class="s-main-product__price">
                {{ selected_variant.price | money_without_trailing_zeros }}
              </span>
            {%- endif -%}
          </div>
          {% comment %} Subtitle {% endcomment %}
          {% if product.description != blank %}
            {% capture read_more_dropdown %}... <span class="s-main-product__read-more js-main-product__open-dropdown">{{ "sections.main-product.see_more" | t }}</span>{% endcapture %}
            {% capture read_less %} <span class="s-main-product__read-more js-main-product__close-dropdown">{{ "sections.main-product.see_less" | t }}</span>{% endcapture %}
            <div class="s-main-product__subtitle">
              <div class="s-main-product__dropdown-text js-main-product__dropdown-close is-show">
                {{ product.description | truncate: 236, read_more_dropdown }}
              </div>
              <div class="s-main-product__dropdown-text js-main-product__dropdown-open">
                {{ product.description | append: read_less }}
              </div>
            </div>
          {% endif %}

          {% comment %} Variant Picker {% endcomment %}
          <div class="s-main-product__variant-picker">
            {% comment %} Form {% endcomment %}
            {% form 'product', product, class: 's-main-product__form js-main-product__form' %}
              {% comment %} Size options {% endcomment %}
              {% assign product_option_color = product.options_by_name.Color %}
              {% assign product_option_size = product.options_by_name.Size %}

              <div class="s-main-product__variant-titles-container">
                {% if product_option_size %}
                  <div class="s-main-product__variant-title">
                    <details class="s-main-product__select">
                      <summary class="s-main-product__select-button">
                        <span class="s-main-product__select-title">{{ 'sections.main-product.size' | t }}</span>
                        <input
                          class="s-main-product__select-input s-main-product__select-input--visible"
                          type="radio"
                          name="product-size_item-qty"
                          id="{{ product.id }}_default-qty"
                          title="{{ selected_variant.options[size_option_position] }}"
                          checked
                        >

                        {% for option in product_option_size.values %}
                          {% liquid
                            assign size_value = option | downcase
                            assign value = option | handleize

                            if selected_variant_color
                              for variant in product.variants
                                if variant.options contains option and variant.options contains selected_variant_color
                                  assign size_variant = variant
                                  break
                                endif
                              endfor
                            else
                              for variant in product.variants
                                if variant.options contains option
                                  assign size_variant = variant
                                  break
                                endif
                              endfor
                            endif
                          %}

                          <input
                            class="s-main-product__select-input js-main-product__variant-size-input {% unless size_variant.available or size_variant %}s-main-product__variant-select-input--disable{% endunless %}"
                            type="radio"
                            id="{{ product.id }}-{{ value }}-{{ forloop.index0 }}"
                            name="product-size"
                            value="{{ size_variant.title }}"
                            data-product-handle="{{ product.handle }}"
                            data-variant-id="{{ size_variant.id }}"
                            data-option-name="size"
                            data-size-name="{{ option }}"
                            {% if selected_variant.id == size_variant.id %}
                              checked
                            {% endif %}
                          >
                        {% endfor %}
                        <span class="s-main-product__select-button-icon">
                          {% render 'icon-arrow-right' %}
                        </span>
                      </summary>
                      <ul class="s-main-product__select-list">
                        {% for option in product_option_size.values %}
                          {% liquid
                            assign value = option | handleize

                            if selected_variant_color
                              for variant in product.variants
                                if variant.options contains option and variant.options contains selected_variant_color
                                  assign size_variant = variant
                                  break
                                else
                                  assign size_variant = false
                                endif
                              endfor
                            else
                              for variant in product.variants
                                if variant.options contains option
                                  assign size_variant = variant
                                  break
                                endif
                              endfor
                            endif
                          %}

                          <li
                            class="s-main-product__select-list-item js-main-product__select-list-item "
                            {% unless size_variant.available or size_variant %}
                              disabled
                            {% endunless %}
                          >
                            <label
                              class="s-main-product__select-list-item-label"
                              for="{{ product.id }}-{{ value }}-{{ forloop.index0 }}"
                            >
                              {{ value }}
                              {% if size_variant.inventory_quantity < 5 %}
                                {% if size_variant.available %}
                                  <span>{{ 'sections.main-product.low_stock' | t }}</span>
                                {% endif %}
                              {% endif %}
                            </label>
                          </li>
                        {% endfor %}
                      </ul>
                    </details>
                  </div>
                {% else %}
                  <details class="s-main-product__select disabled">
                    <summary class="s-main-product__select-button">
                      <span class="s-main-product__select-title">{{ 'sections.main-product.size' | t }}</span>
                      <input
                        class="s-main-product__select-input s-main-product__select-input--visible"
                        type="radio"
                        name="product-size_item-qty"
                        id="{{ product.id }}_default-qty"
                        title="{{ 'sections.main-product.one_size' | t }}"
